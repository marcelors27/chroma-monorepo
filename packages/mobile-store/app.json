{
  "expo": {
    "name": "Chroma Mobile Store",
    "slug": "chroma-mobile-store",
    "version": "0.1.1",
    "orientation": "portrait",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "statusBar": {
      "style": "light",
      "backgroundColor": "#000000"
    },
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.chroma.mobilestore",
      "statusBar": {
        "barStyle": "light-content",
        "backgroundColor": "#000000"
      }
    },
    "ios": {
      "bundleIdentifier": "com.chroma.mobilestore",
      "buildNumber": "2",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIStatusBarStyle": "UIStatusBarStyleLightContent",
        "UIViewControllerBasedStatusBarAppearance": false
      }
    },
    "plugins": [
      "expo-asset",
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "9952dc58-f0f3-4fcd-8fcc-3e5c2fa60a7a"
      }
    },
    "owner": "marcelors27"
  }
}
