{
  "expo": {
    "name": "Chroma Mobile Front V2",
    "slug": "chroma-mobile-front-v2",
    "version": "0.1.0",
    "orientation": "portrait",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.chroma.mobilefrontv2"
    },
    "ios": {
      "bundleIdentifier": "com.chroma.mobilefrontv2",
      "buildNumber": "1",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "plugins": [
      "expo-asset",
      "expo-font"
    ]
  }
}
